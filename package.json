{
  "name": "task-queue-manager",
  "version": "1.0.0",
  "description": "Distributed task queue management system with real-time monitoring and analytics",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "api:dev": "nodemon src/api/server.js",
    "api:build": "tsc src/api/server.ts --outDir dist",
    "api:start": "node dist/server.js",
    "docker:build": "docker build -t task-queue-manager .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "test": "jest",
    "test:watch": "jest --watch",
    "analyze": "ANALYZE=true npm run build"
  },
  "keywords": [
    "task-queue",
    "job-processing",
    "distributed-systems",
    "redis",
    "bull-queue",
    "real-time",
    "monitoring",
    "analytics",
    "nextjs",
    "typescript",
    "nodejs",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^2.30.0",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/task-queue-manager.git"
  },
  "bugs": {
